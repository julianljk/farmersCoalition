<%= form_for :user, url: users_path do |f| %>
  <p>
    <%= f.label :email %><br>
    <%= f.text_field :email %>
  </p>
  <p> Password:</br> <%= f.password_field :password%></p>
  <p> Password Confirmation:</br> <%= f.password_field :password_confirmation%> </p> 
  <p>
    <%= f.label :name %><br>
    <%= f.text_field :name %>
  </p>
 
  <p>
    <%= f.label :city %><br>
    <%= f.text_field :city %>
  </p>
 
  <p>
    <%= f.submit :Signup %>
  </p>
<% end %>